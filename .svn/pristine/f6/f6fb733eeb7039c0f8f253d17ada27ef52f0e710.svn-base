<%= link_to %(<i class="glyphicon glyphicon-search"></i> #{t("views.proceedings.edit.button.search")}).html_safe, meetings_path, class: "btn btn-default btn-meeting-search", target: "_blank", "tooltip" => t('views.proceedings.edit.balloon_help.search_meeting'), "tooltip-placement" => "right", "tooltip-popup-delay" => BALLOON_HELP_POPUP_DELAY %>
<%= link_to t("views.proceedings.edit.button.prev"), "/meetings/#{@proceeding.meeting.id}", class: "btn btn-default btn-prev", "tooltip" => t('views.proceedings.edit.balloon_help.back'), "tooltip-placement" => "top", "tooltip-popup-delay" => BALLOON_HELP_POPUP_DELAY %>
<%= link_to t("views.proceedings.edit.button.confirm-end"), "/proceedings/#{@proceeding.id}/confirm", class: "btn btn-default btn-submit", data: { "check-target" => ".agenda_content", "content-is-blank" => t('views.proceedings.edit.error-message.content-is-blank') }, "check-agenda" => '', "tooltip" => t('views.proceedings.edit.balloon_help.meeting_confirm'), "tooltip-placement" => "top" %>
