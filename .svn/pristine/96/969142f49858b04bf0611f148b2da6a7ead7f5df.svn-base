<div id="enable-email" class="email-div">
  <div class="main-header">
    <h3><%= t('views.cybozus.send_email.title') %></h3>
  </div>

  <div class="form-control">
    <div class="span12">
      <% if (@admin_setting[:enable_cybozu] == true) %>
          <input type="checkbox" checked="checked" id="enableSendEmail" name="enableSendEmail"/>&nbsp;<%= t('views.cybozus.send_email.enable') %>
      <% else  %>
          <input type="checkbox"  id="enableSendEmail" name="enableSendEmail"/>&nbsp;<%= t('views.cybozus.send_email.enable') %>
      <% end  %>
    </div>
  </div>


</div>
